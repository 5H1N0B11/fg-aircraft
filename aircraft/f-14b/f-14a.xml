          <table>
            <independentVar lookup="row">aero/alpha-rad</independentVar>
            <tableData>
0	0.06145
0.087266444	0.5286301
0.174532889	0.9215569
0.261799333	1.213586
0.349065778	1.439421
0.436332222	1.64045
0.523598667	1.720108
0.610865111	1.733031
0.698131556	1.666409
0.785398	1.567197
0.872664444	1.476011
0.959930889	1.346354
            </tableData>
          </table>
      </product>
    </function>

    <function name="aero/force/Lift_flap">
