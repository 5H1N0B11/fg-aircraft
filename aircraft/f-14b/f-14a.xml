  Inputs: (based on aeromatic - modified using reference[1])
    name:          F14A
    type:          two-engine transonic/supersonic fighter
    max weight:    74350 lb
    wing span:     64 ft
    length:        62.75 ft
    wing area:     54.5 sq-ft
    gear type:     tricycle
    retractable?:  yes
    # engines:     2
    engine type:   turbine
    engine layout: middle fuselage
    yaw damper?    yes
  Outputs:
    wing loading:  1364.22 lb/sq-ft
    payload:       58954.8 lbs
