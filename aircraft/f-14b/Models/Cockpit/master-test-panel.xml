<PropertyList>

	<path>master-test-panel.ac</path>

	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>emerg-flt-hyd-guard-lever</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>sim/model/f-14b/controls/electrics/emerg-flt-hyd-guard-lever</property>
			</binding>
			<binding>
    			<command>property-assign</command>
				<property>sim/model/f-14b/controls/electrics/emerg-flt-hyd-switch</property>
				<value>0</value>
			</binding>
		</action>
	</animation>

	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>master-test-switch</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>sim/model/f-14b/controls/electrics/master-test-switch</property>
			</binding>
		</action>
	</animation>

	<animation>
		<type>pick</type>
		<visible>false</visible>

		<object-name>hotspot-emerg-flt-hyd-low</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
    			<command>property-assign</command>
				<property>sim/model/f-14b/controls/electrics/emerg-flt-hyd-switch</property>
				<value>1</value>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<visible>false</visible>

		<object-name>hotspot-emerg-flt-hyd-high</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
    			<command>property-assign</command>
				<property>sim/model/f-14b/controls/electrics/emerg-flt-hyd-switch</property>
				<value>2</value>
			</binding>
		</action>
	</animation>
	<animation>
		<type>pick</type>
		<visible>false</visible>

		<object-name>hotspot-emerg-flt-hyd-auto</object-name>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
    			<command>property-assign</command>
				<property>sim/model/f-14b/controls/electrics/emerg-flt-hyd-switch</property>
				<value>0</value>
			</binding>
		</action>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>master-test-switch</object-name>
		<property>sim/model/f-14b/controls/electrics/master-test-switch</property>
		<factor>-50</factor>
		<center>
			<x-m>-0.0057</x-m>
			<y-m>-0.0142</y-m>
			<z-m> 0.0231</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
	</animation>


	<animation>
		<type>rotate</type>
		<object-name>emerg-flt-hyd-switch</object-name>
		<property>sim/model/f-14b/controls/electrics/emerg-flt-hyd-switch</property>
		<interpolation>
			<entry><ind>0</ind><dep>50</dep></entry>
			<entry><ind>1</ind><dep>0</dep></entry>
			<entry><ind>2</ind><dep>-50</dep></entry>
		</interpolation>
		<center>
			<x-m>-0.0090</x-m>
			<y-m> 0.0631</y-m>
			<z-m> 0.0054</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>emerg-flt-hyd-guard-lever</object-name>
		<property>sim/model/f-14b/controls/electrics/emerg-flt-hyd-guard-lever</property>
		<factor>90</factor>
		<center>
			<x-m>-0.0295</x-m>
			<y-m> 0.0606</y-m>
			<z-m> 0.0076</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

</PropertyList>
