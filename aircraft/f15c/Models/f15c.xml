    <!-- ******************************************************************* -->
    <!-- Cockpit                                                             -->
    <!-- ******************************************************************* -->
    <model>
        <path>Aircraft/f15c/Models/Cockpit/cockpit.xml</path>
        <offsets>
            <x-m>0</x-m>
            <y-m>0 </y-m>
            <z-m>0</z-m>
            <pitch-deg>0</pitch-deg>
        </offsets>
    </model>

    <animation>
        <type>noshadow</type>
        <object-name>ExternalFlame</object-name>
        <object-name>InternalFlame</object-name>
    </animation>

    <effect>
        <inherits-from>Effects/model-transparent</inherits-from>
        <object-name>Glass_Canopy01</object-name>
        <object-name>Glass_HUD</object-name>
        <object-name>Glass01</object-name>
    </effect>

    <animation>
        <type>select</type>
        <object-name>FrontWheel</object-name>
        <object-name>FrontWheelTire</object-name>
        <condition>
            <greater-than>
                <property>gear/gear[0]/position-norm</property>
                <value>0</value>
            </greater-than>
        </condition>
    </animation>
	<animation>
		<type>translate</type>
		<object-name>low fwd oleo</object-name>
		<object-name>Nose wheel</object-name>
		<object-name>Cylinder</object-name>
		<property>gear/gear[0]/compression-norm</property>
		<interpolation>
			<entry><ind>0</ind><dep>-0.1</dep></entry>
            <entry>
                <ind>1.0</ind>
                <dep>0.6</dep>
            </entry>
        </interpolation>
		<axis>
			<x>0.098</x>
			<y>0</y>
			<z>1.13</z>
		</axis>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>Main lower oleo</object-name>
		<object-name>Main  wheel</object-name>
		<property>gear/gear[1]/compression-norm</property>
        <offset-m>-0.1</offset-m>
        <factor></factor>
		<interpolation>
            <entry>
                <ind>0</ind>
                <dep>-0.1</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>0.3</dep>
            </entry>
        </interpolation>
		<axis>
			<x>0.098</x>
			<y>0</y>
			<z>1.13</z>
		</axis>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>Main lower oleo.001</object-name>
		<object-name>Main  wheel.001</object-name>
		<property>gear/gear[2]/compression-norm</property>
		<interpolation>
            <entry>
                <ind>0</ind>
                <dep>-0.1</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>0.3</dep>
            </entry>
            </interpolation>
		<axis>
			<x>0.098</x>
			<y>0</y>
			<z>1.13</z>
		</axis>
	</animation>

<!-- wheel rotation -->
	<animation>
		<type>spin</type>
		<object-name>Nose wheel</object-name>
		<property>gear/gear[0]/rollspeed-ms</property>
		<factor>7.0</factor>
		<center>
			<x-m>-3.284</x-m>
			<y-m>0.0792</y-m>
			<z-m>-1.6948</z-m>
		</center>
		<axis>
			<x>0.0</x>
			<y>-1.76</y>
			<z>0.013</z>
		</axis>
	</animation>
	<animation>
		<type>spin</type>
		<object-name>Main  wheel</object-name>
		<property>gear/gear[1]/rollspeed-ms</property>
		<factor> -7.0 </factor>
		<center>
			<x-m>2.0036</x-m>
			<y-m>-1.53596</y-m>
			<z-m>-1.60016</z-m>
		</center>
		<axis>
			<x>-9.5604938271604938271604938271605e-5</x>
			<y>1.76</y>
			<z>-0.082</z>
		</axis>
	</animation>
	<animation>
		<type>spin</type>
		<object-name>Main  wheel.001</object-name>
		<property>gear/gear[2]/rollspeed-ms</property>
		<factor>5.8</factor>
		<center>
			<x-m>2.0036</x-m>
			<y-m>1.58591</y-m>
			<z-m>-1.60016</z-m>
		</center>
		<axis>
            <x>9.5604938271604938271604938271605e-5</x>
            <y>1.76</y>
            <z>0.082</z>
        </axis>
	</animation>

    <animation>
        <type>rotate</type>
        <object-name>FrontWheel</object-name>
        <object-name>FrontWheelTire</object-name>
        <property>gear/gear[0]/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>-110</dep>
            </entry>
            <entry>
                <ind>0.1</ind>
                <dep>-110</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0</dep>
            </entry>
        </interpolation>
        <center>
            <x-m>-3.0</x-m>
            <y-m>0.0</y-m>
            <z-m>-0.1</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>
    <!-- inlet ramps-->
    <animation>
        <type>rotate</type>
        <object-name>Intake ramp</object-name>
        <object-name>IntakeLipL</object-name>
        <property>fdm/jsbsim/propulsion/inlet/ramp1-position-deg</property>
        <center>
            <x-m>-2.06031</x-m>
            <y-m>0.87825</y-m>
            <z-m>-0.16711</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

    <!--gear -->

    <animation>
        <type>select</type>
        <object-name>fwd retract strut</object-name>
        <object-name>Cube.004</object-name>
        <object-name>Cylinder</object-name>
        <object-name>Upper FWD oleo</object-name>
        <object-name>Landing light</object-name>
        <object-name>Cylinder.003</object-name>
        <object-name>Nose wheel</object-name>
        <object-name>Main retract strut</object-name>
        <object-name>main upper oleo</object-name>
        <object-name>Main lower oleo</object-name>
        <object-name>Cylinder.002</object-name>      
        <object-name>Main  wheel</object-name>
        <object-name>Main retract stru</object-name>
        <object-name>main upper oleo.001</object-name>
        <object-name>Main lower oleo.001</object-name>
        <object-name>Cylinder.001</object-name>      
        <object-name>Main  wheel.001</object-name>            
        <condition>
            <greater-than>
                <property>gear/gear[2]/position-norm</property>
                <value>0</value>
            </greater-than>
        </condition>
    </animation>


<!-- speed brake -->

    <animation>
        <type>rotate</type>
        <object-name>Airbrake</object-name>
        <object-name>AirbrakeInside</object-name>
        <property>surface-positions/speedbrake-pos-norm</property>
		<factor>-45</factor>
        <center>
            <x-m>-0.7927</x-m>
            <y-m>0.0206</y-m>
            <z-m>1.1368</z-m>
        </center>
        <axis>
            <x>0.0</x>
            <y>1.0</y>
            <z>0</z>
        </axis>
    </animation>
    
