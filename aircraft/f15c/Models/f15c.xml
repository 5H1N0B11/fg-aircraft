  <effect>
  <name>glossy</name>
   <inherits-from>Aircraft/f15c/Models/metal</inherits-from>
   <object-name>EngineCovers</object-name>
   <object-name>Heat Xchanger.001</object-name>
</effect>
  
<animation>
		<type>shader</type>
		<shader>chrome</shader>
		<texture>Aircraft/f15c/Models/glass_shader.png</texture>
		<object-name>Nose Section.003</object-name>
		<object-name>Canopy.002</object-name>
</animation>

  
    <!-- Poussière -->
