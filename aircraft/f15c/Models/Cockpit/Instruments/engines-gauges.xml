    <!-- temperatures -->
    <animation>
        <name>temp-l</name>
        <type>rotate</type>
        <object-name>needle-temp-l</object-name>
        <property>engines/engine[0]/egt-degC</property>
        <interpolation>
            <entry>
                <ind>200</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>1400</ind>
                <dep>235</dep>
            </entry>
        </interpolation>
        <center>
            <x-m> 0.0012</x-m>
            <y-m> -0.0409</y-m>
            <z-m> 0.0029</z-m>
        </center>
        <axis>
            <x>-1</x>
