                    <animation>
        <type>pick</type>
        <visible>true</visible>
        <object-name>misc-ldg-light-switch</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>nasal</command>
                <script>
                    if(getprop("sim/multiplay/generic/int[7]") == 0)
                        setprop("sim/multiplay/generic/int[7]",2);
                        elsif(getprop("sim/multiplay/generic/int[7]") == 2)
                        setprop("sim/multiplay/generic/int[7]",1);
                        elsif(getprop("sim/multiplay/generic/int[7]") == 1)
                        setprop("sim/multiplay/generic/int[7]",0);
                </script>
            </binding>
        </action>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>misc-ldg-light-switch</object-name>
        <property>sim/multiplay/generic/int[7]</property> <!-- This is bit 0 for taxi, bit 1 for landing. As the switch is 3way 0,1,2 is fine - if there were more settings it'd be harder-->
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>30</dep>
            </entry>
            <entry>
                <ind>2</ind>
                <dep>-30</dep>
            </entry>
        </interpolation>
        <center>
            <x-m>-5.6611</x-m>
            <y-m>-0.4441</y-m>
            <z-m>0.6541</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

