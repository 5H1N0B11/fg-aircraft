<?xml version="1.0"?>


<PropertyList>

	<path>left-side-panel.ac</path>
<!-- generators -->
    <animation>
        <type>pick</type>
        <visible>true</visible>
        <object-name>eng-gen-left-switch</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>sim/model/f15/controls/electrics/l-gen-switch</property>
            </binding>
        </action>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>eng-gen-left-switch</object-name>
        <property>sim/model/f15/controls/electrics/l-gen-switch</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>-50</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0</dep>
            </entry>
        </interpolation>
        <center>
            <x-m>-5.65485</x-m>
            <y-m> 0.30604</y-m>
            <z-m> 0.68</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>

