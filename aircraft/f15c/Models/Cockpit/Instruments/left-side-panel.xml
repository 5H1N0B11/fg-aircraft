    <animation>
        <type>pick</type>
        <visible>true</visible>
        <object-name>l-eng-master-guard</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>sim/model/f15/controls/engs/l-eng-master-guard</property>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <visible>true</visible>
        <object-name>l-eng-master-switch</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>nasal</command>
                <script>
                    if(getprop("sim/model/f15/controls/engs/l-eng-master-guard"))
                    {
                    if (!getprop("engines/engine[0]/running") and  getprop("controls/engines/engine[0]/cutoff"))
                    {
                    setprop("controls/engines/engine[0]/starter",1);
                    print("Set crank L");
                    settimer(func { setprop("controls/engines/engine[0]/cutoff",0) }, 0.2);

                    }
                    else setprop("controls/engines/engine[0]/cutoff",1-getprop("controls/engines/engine[0]/cutoff"));
                    }
                </script>
            </binding>
        </action>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>l-eng-master-switch</object-name>
        <property>controls/engines/engine[0]/cutoff</property>
        <factor>-70</factor>
        <center>
            <x-m>-5.617</x-m>
            <y-m> 0.3089</y-m>
            <z-m> 0.67669</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
