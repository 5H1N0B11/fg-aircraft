<?xml version="1.0"?>

<!-- F-15 C (MSIP) stick by Richard Harrison; rjh@zaretto.com -->

<PropertyList>
    <path>stick.ac</path>

    <animation>
        <type>rotate</type>
        <object-name>stick-handle</object-name>
        <object-name>stick-mast</object-name>
        <property>controls/flight/elevator</property>
        <factor>18</factor>
        <center>
            <x-m>-5.5126</x-m>
            <y-m>-0.0468</y-m>
            <z-m> 0.3764</z-m>
        </center>

        <axis>
            <y>-1</y>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>stick-handle</object-name>
        <property>controls/flight/aileron</property>
        <factor>20</factor>
        <center>
            <x-m>-5.5126</x-m>
            <y-m> 0.0139</y-m>
            <z-m> 0.6024</z-m>
        </center>

        <axis>
            <x>-1</x>
        </axis>
    </animation>


    <!-- *** LoD *** -->
    <animation>
        <type>range</type>
        <min-m>0</min-m>
        <max-m>35</max-m>
    </animation>

</PropertyList>
