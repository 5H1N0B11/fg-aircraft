            <key n="109">
                <name>m</name>
                <desc>Cycle Weapon Mode Selector</desc>
                <binding>
                    <command>property-adjust</command>
                    <property>sim/model/f15/controls/armament/weapon-selector</property>
                    <step>1</step>
                    <min>0</min>
                    <max>3</max>
                    <repeat>false</repeat>
                    <wrap>true</wrap>
                </binding>
            </key>
            <key n="121">
                <name>y</name>
                <desc>Next target select</desc>
                <binding>
                    <command>property-assign</command>
