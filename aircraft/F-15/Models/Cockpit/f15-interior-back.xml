<?xml version="1.0"?>

<!-- F-15 interior backseat stick by Richard Harrison; rjh@zaretto.com -->

<PropertyList>
    <path>f15-interior-back.ac</path>
    <animation>
        <type>select</type>
        <condition>
            <equals>
                <property>fdm/jsbsim/metrics/two-place-canopy</property>
                <value>1</value>
            </equals>
        </condition>
        <object-name>interior-back</object-name>
    </animation>
</PropertyList>
